{
  "name": "sostav",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "adb-nox": "adb connect localhost:62025",
    "adb-mobile": "adb tcpip 5555",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "android-build-debug": "cd android/ && ./gradlew assembleDebug",
    "android-build-release": "cd android && ./gradlew assembleRelease",
    "android-clean": "cd android && ./gradlew clean",
    "android-run-debug": "react-native run-android",
    "android-run-release": "react-native run-android --variant=release",
    "android-getKey": "keytool -list -v -keystore ./android/app/release.keystore -alias lettuce -storepass lettuce -keypass lettuce",
    "android-signkey": "keytool -genkey -v -keystore release.keystore -alias lettuce -keyalg RSA -keysize 2048 -validity 10000",
    "android-signingReport": "cd android/ && ./gradlew signingReport",
    "reset-cache": "react-native start --reset-cache"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/datetimepicker": "^3.3.0",
    "@react-native-firebase/app": "^11.1.2",
    "@react-native-firebase/auth": "^11.1.2",
    "@react-native-firebase/database": "^11.1.2",
    "@react-native-firebase/messaging": "^11.1.2",
    "@react-native-firebase/storage": "^11.1.2",
    "@react-navigation/bottom-tabs": "^5.11.8",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-confirmation-code-field": "^6.5.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-image-picker": "^3.3.2",
    "react-native-masked-text": "^1.13.0",
    "react-native-modal": "^11.7.0",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.18.1",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-swipe-list-view": "^3.2.6",
    "react-native-switch-pro": "https://github.com/redspider926/react-native-switch-pro.git",
    "react-native-toast-message": "^1.4.9",
    "react-navigation-backhandler": "^2.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/runtime": "^7.13.10",
    "@react-native-community/eslint-config": "^2.0.0",
